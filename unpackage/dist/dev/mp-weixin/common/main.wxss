.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
page {
  background-image: url('data:image/png;base64,');
  background-position: top;
  background-size: 360px 360px;
  background-color: #191919;
}
.stripe-background {
  background-image: linear-gradient(180deg, transparent, rgba(36, 36, 36, 0.1)), url('data:image/png;base64,');
  background-size: 100% 100%, 80px 80px;
  background-color: gray;
}
.stripe-background.regular {
  background-color: #19d719;
}
.stripe-background.regular.card-hook:before {
  background-color: #19d719;
}
.stripe-background.gachi {
  background-color: #f54910;
}
.stripe-background.gachi.card-hook:before {
  background-color: #f54910;
}
.stripe-background.league {
  background-color: #f02d7d;
}
.stripe-background.league.card-hook:before {
  background-color: #f02d7d;
}
.stripe-background.splatfest {
  background-color: #aaaaaa;
}
.stripe-background.splatfest.card-hook:before {
  background-color: #aaaaaa;
}
.dot-background {
  background-image: url(data:image/png;base64,);
  background-color: gray;
}
.circle-background {
  background-image: url(data:image/png;base64,);
  background-color: #ff5600;
}
.sports-background {
  background-image: url(data:image/png;base64,);
  background-color: gray;
}
.wave-background {
  background-image: url(data:image/png;base64,);
  background-color: gray;
  background-size: 140px;
}
.card-mask {
  -webkit-mask-image: url(data:image/png;base64,);
  -webkit-mask-repeat: no-repeat;
  -webkit-mask-position: center top;
  -webkit-mask-size: 1200rpx 1920rpx;
}
.card-hook {
  position: relative;
}
.card-hook:before {
  content: '';
  position: absolute;
  background-position-y: bottom !important;
  width: 100%;
  height: 44rpx;
  top: -40rpx;
  left: 0;
  z-index: -1;
  background-color: gray;
  -webkit-mask-image: url(data:image/png;base64,);
  -webkit-mask-size: 54rpx 44rpx;
  -webkit-mask-position: center top;
  -webkit-mask-repeat: no-repeat;
}
.block-banner {
  -webkit-mask-image: url(data:image/png;base64,);
  -webkit-mask-size: 100% 100%;
  -webkit-mask-position: center;
  -webkit-mask-repeat: no-repeat;
}
.ink-mask {
  -webkit-mask-image: url(data:image/png;base64,);
  -webkit-mask-size: 100% 100%;
  -webkit-mask-position: center;
  -webkit-mask-repeat: no-repeat;
}
@font-face {
  font-family: 'splatoon2';
  src: url('data:application/font-woff;charset=utf-8;base64,') format('woff');
}
@font-face {
  font-family: 'Splatfont';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff');
}
.splatoon {
  font-family: Splatfont, BlinkMacSystemFont, -apple-system, Helvetica Neue, Helvetica, PingFang SC, Hiragino Sans GB, Microsoft YaHei, Arial, sans-serif;
  text-shadow: 2rpx 2rpx 0 #000;
}
.splatoon2 {
  font-family: splatoon2, BlinkMacSystemFont, -apple-system, Helvetica Neue, Helvetica, PingFang SC, Hiragino Sans GB, Microsoft YaHei, Arial, sans-serif;
  text-shadow: 2rpx 2rpx 0 #000;
}
.card-wrap {
  width: 90%;
  box-shadow: 0 4rpx 16rpx rgba(0, 0, 0, 0.2);
  padding: 20rpx;
  border-radius: 30rpx;
  position: relative;
  -webkit-transform: rotate(0.2deg);
          transform: rotate(0.2deg);
}
.card-title {
  display: flex;
  align-items: center;
}
page {
  background-color: #19232C;
  font-weight: bolder;
  color: white;
}
.hr {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  height: 0;
  border-bottom: 2rpx dashed #ffffff;
}
.img {
  height: auto;
}

